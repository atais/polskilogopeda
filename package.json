{
    "name": "polskilogopeda",
    "description": "",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "npm run clean && webpack && bundle exec jekyll build",
        "start": "npm run clean && concurrently 'npm run jekyll-dev' 'npm run webpack-dev'",
        "webpack-dev": "webpack --watch --progress",
        "jekyll-dev": "bundle exec jekyll serve --watch --host 0.0.0.0",
        "clean": "rimraf _site",
        "deploy": "npm run build && git-directory-deploy --directory dist --branch gh-pages"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "5.13.0",
        "bootstrap": "4.5.0",
        "jquery": "3.5.1",
        "jquery.easing": "1.4.1",
        "magnific-popup": "1.1.0",
        "popper.js": "1.16.1"
    },
    "devDependencies": {
        "concurrently": "5.2.0",
        "css-loader": "3.5.3",
        "file-loader": "6.0.0",
        "git-directory-deploy": "1.5.1",
        "mini-css-extract-plugin": "0.9.0",
        "rimraf": "3.0.2",
        "sass": "1.26.5",
        "sass-loader": "8.0.2",
        "url-loader": "4.1.0",
        "webpack": "4.43.0",
        "webpack-cli": "3.3.11"
    }
}
